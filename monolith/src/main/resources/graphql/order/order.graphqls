type Order {
    id: ID!
    tutor: User!
    student: User!
    course: Course!
    isActive: Boolean!
}

input OrderInput{
    orderId: ID!
    tutorId: ID!
}

extend type Query {
    getAllOrder:[Order]
}

extend type Mutation{
    createOrder(courseInput: CourseInput): Order
    deleteOrder(orderId: Int!): Order
    assignTutor(orderInput: OrderInput): Order
    cancelJobApplication(orderInput: OrderInput): Order
}
